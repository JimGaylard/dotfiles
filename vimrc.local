set shortmess+=I

" Key remapping
inoremap jk <Esc>

if has("autocmd")
    autocmd BufWritePost .vimrc.local source $MYVIMRC
endif

nmap <leader>v :tabedit $MYVIMRC.local<CR>
nmap <leader><leader>v :tabedit $MYVIMRC<CR>
set clipboard=unnamed

set background=dark

set shiftwidth=2
set softtabstop=2
set tabstop=2
autocmd FileType python setlocal shiftwidth=4 softtabstop=4

UnBundle 'godlygeek/csapprox'
